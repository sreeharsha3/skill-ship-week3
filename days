
day-1

1.gcd and lcm code

#include <iostream>
using namespace std;
long gcd(long,long); //declared

//def
long gcd(long a,long b){
    if(b==0){
        return a;
    }
    else{
        return gcd(b,a%b);
    }
}

int main() {
	int t;
	cin>>t;
	while(t--){
	    
	    long a,b,lcm,gcdd;
	    cin>>a>>b;
	    gcdd=gcd(a,b);
	    lcm=(a*b)/gcdd;
	    cout<<lcm<<" "<<gcdd<<endl;
	    
	}
	return 0;
}



